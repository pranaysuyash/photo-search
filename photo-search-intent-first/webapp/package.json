{
	"name": "photo-search-intent-first-web",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"test": "vitest run --config vitest.config.ts",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:noop": "echo 'No tests yet' && exit 0",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:visual": "playwright test tests/visual/ --update-snapshots=missing",
		"test:visual:update": "playwright test tests/visual/ --update-snapshots",
		"test:visual:ci": "playwright test tests/visual/ --fail-on-changed-snapshots",
		"test:offline": "playwright test tests/offline-comprehensive.test.ts tests/offline-pwa.test.ts",
		"test:offline:ci": "playwright test tests/offline-comprehensive.test.ts tests/offline-pwa.test.ts --reporter=list",
		"test:offline:debug": "playwright test tests/offline-comprehensive.test.ts tests/offline-pwa.test.ts --debug",
		"test:offline:mobile": "playwright test tests/offline-comprehensive.test.ts --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
		"test:offline:performance": "playwright test tests/offline-comprehensive.test.ts --reporter=html,json",
		"analyze": "npm run build && node analyze-bundle.js",
		"build:analyze": "vite build --mode analyze",
		"lint": "npm run lint:auto && npx @biomejs/biome check src",
		"lint:fix": "npx @biomejs/biome check src --write",
		"lint:fix:unsafe": "npx @biomejs/biome check src --write --unsafe",
		"lint:auto": "node fix-button-types.mjs && node fix-typescript-types.mjs && node fix-accessibility.mjs",
		"lint:eslint": "eslint src --ext .ts,.tsx",
		"lint:eslint:fix": "eslint src --ext .ts,.tsx --fix",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"@floating-ui/react": "^0.27.16",
		"@radix-ui/react-checkbox": "^1.3.3",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-select": "^2.2.6",
		"@radix-ui/react-separator": "^1.1.7",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-switch": "^1.2.6",
		"@radix-ui/react-tabs": "^1.1.13",
		"@radix-ui/react-toast": "^1.2.15",
		"@tanstack/react-query": "^5.90.1",
		"@tensorflow/tfjs": "^4.22.0",
		"@types/leaflet": "^1.9.20",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"framer-motion": "^12.23.12",
		"leaflet": "^1.9.4",
		"lucide-react": "^0.453.0",
		"onnxruntime-web": "^1.23.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-leaflet": "^5.0.0",
		"react-router-dom": "^6.23.1",
		"react-scan": "^0.4.3",
		"react-virtualized-auto-sizer": "^1.0.26",
		"react-window": "^2.1.2",
		"tailwind-merge": "^3.3.1",
		"uuid": "^13.0.0",
		"zod": "^3.25.76",
		"zustand": "^5.0.8"
	},
	"devDependencies": {
		"@playwright/test": "^1.55.0",
		"@storybook/addon-a11y": "^9.1.6",
		"@storybook/addon-docs": "^9.1.6",
		"@storybook/addon-onboarding": "^9.1.6",
		"@storybook/react-vite": "^9.1.6",
		"@testing-library/jest-dom": "^6.4.8",
		"@testing-library/react": "^14.3.1",
		"@types/node": "^24.3.1",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vitejs/plugin-react": "^4.2.0",
		"@vitest/coverage-v8": "^1.6.0",
		"autoprefixer": "^10.4.18",
		"eslint": "^8.57.0",
		"eslint-plugin-boundaries": "^4.2.0",
		"glob": "^11.0.3",
		"jsdom": "^24.0.0",
		"msw": "^2.11.3",
		"playwright": "^1.55.0",
		"postcss": "^8.4.35",
		"storybook": "^9.1.6",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.3.3",
		"vite": "^5.0.0",
		"vitest": "^1.6.0"
	}
}
